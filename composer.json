{
  "name": "larajs/i18n",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
  "keywords": [
    "laravel",
    "vue-i18n"
  ],
  "authors": [
    {
      "name": "Tanmnt",
      "email": "maingocthanhtan96@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "illuminate/console": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|~7.0|~8.0|~9.0|~10.0",
    "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|~7.0|~8.0|~9.0|~10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0"
  },
  "autoload": {
    "psr-4": {
      "LaraJS\\I18n\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaraJS\\I18n\\GeneratorProvider"
      ]
    }
  }
}
